                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/bluearrow2"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"/>

                </LinearLayout>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
